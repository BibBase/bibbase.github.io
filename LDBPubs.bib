@Article{Carey2009,
  author          = {Carey, Timothy S. and Freburger, Janet K. and Holmes, George M. and Castel, Liana and Darter, Jane and Agans, Robert and Kalsbeek, William and Jackman, Anne},
  title           = {A long way to go: practice patterns and evidence in chronic low back pain care.},
  doi             = {10.1097/BRS.0b013e31819792b0},
  issn            = {1528-1159},
