@article{vanrooy2018regionalresearch,
author = {Van Rooy, R},
journal = {Beitrage zur Geschichte der Sprachwissenschaft},
month = {Dec},
number = {2},
pages = {167--216},
publisher = {Nodus-Publ. Munster Dutz},
title = {Regional language variation in European thought before 1500: A historical sketch reflecting current research},
volume = {28},
year = {2018},
abstract = {Awareness of, and reflection on, regional variation within a language have a long history, which receives increasing attention from modern scholars. The present contribution aims to systematize the dispersed available literature by offering a historical sketch of views on regional language variation in pre-1500 European thought that reflects the research of the past few decades. It is primarily based on secondary literature, but complemented by new research findings wherever possible. The focus of attention is on the Greco-Roman tradition and the Western Middle Ages. The main objective of this paper consists in stimulating more systematic and more organized research into the history of dialect studies, a subfield of the historiography of linguistics that is largely understudied. It aims to do so by means of a synthetic overview of Western ideas on regional language variation before 1500, which constituted the basis for later insights on the phenomenon.},
issn = {0939-2815},
day = {15},
publicationstatus = {published},
}
